a=list(map(int,input().split()))
b=a[1]
a=a[0]
p=a
m=[]
for i in range(a):
    print(p,end=" ")
    m.append(p)
    p=p-1
print("")
t=[]
for i in range(b-1):
    if i%2!=0:
        for j in range(a-1,-1,-1):
            print(2*t[j],end=" ")
            m.append(2*t[j])
        t=[]
    else:
        for j in range(a-1,-1,-1):
            print(2*m[j],end=" ")
            t.append(2*m[j])
        m=[]
    print()
        
